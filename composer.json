{
    "name": "revinate/analytics-bundle",
    "description": "Revinate Analytics Bundle",
    "keywords": ["symfony2", "elasticsearch", "analytics"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [{
        "name" : "Vinay Kumar"
    }],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "amnl/gc-datatable",
                "version": "0.2.0",
                "dist": {
                    "url": "https://github.com/itavero/AMNL-GCDataTable/archive/master.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "php"                         : ">=5.3.0",
        "symfony/yaml"                : ">=2.4.8",
        "symfony/dependency-injection": ">=2.4.8",
        "symfony/config"              : ">=2.4.8",
        "ruflin/elastica"             : "1.2.1.0",
        "amnl/gc-datatable": "dev-master"
    },
    "require-dev": {
        "symfony/console": "~2.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "autoload": {
        "psr-0": {
            "Revinate\\AnalyticsBundle\\": ""
        }
    },
    "target-dir" : "Revinate/AnalyticsBundle"
}

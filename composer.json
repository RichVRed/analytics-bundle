{
    "name": "revinate/analytics-bundle",
    "description": "Revinate Analytics Bundle",
    "keywords": ["symfony2", "elasticsearch", "analytics"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [{
        "name" : "Vinay Kumar"
    }],
    "repositories": [
        {
            "name": "ruflin/elastica",
            "type": "git",
            "url": "https://github.com/revinate/Elastica"
        }
    ],
    "require": {
        "php"                         : ">=5.3.0",
        "symfony/yaml"                : ">=2.4.8",
        "symfony/dependency-injection": ">=2.4.8",
        "symfony/config"              : ">=2.4.8",
        "ruflin/elastica"             : "2.2.0",
        "amnl/gc-datatable"           : "dev-master"
    },
    "require-dev": {
        "symfony/console": ">=2.4.8",
        "symfony/framework-bundle": ">=2.4.8",
        "phpunit/phpunit": ">=4.6.9",
        "symfony/finder": ">=2.4.8",
        "symfony/process": ">=2.4.8",
        "symfony/browser-kit": ">=2.4.8",
        "doctrine/orm": ">=2.4"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "autoload": {
        "psr-0": {
            "Revinate\\AnalyticsBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-0": {
          "Revinate\\AnalyticsBundle\\Test\\": "Test/app"
        }
    },
    "target-dir" : "Revinate/AnalyticsBundle"
}
